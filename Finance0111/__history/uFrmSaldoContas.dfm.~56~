      '     CB.ID_CONTA_CONTABIL'
      ''
      ''
      'FROM CONTA_BANCARIA CB'
      'WHERE (CB.ID_ORGANIZACAO = :PIDORGANIZACAO)'
      ''
      '')
    Left = 568
    Top = 544
    ParamData = <
      item
        Name = 'PIDORGANIZACAO'
