      'FROM CONTA_BANCARIA_DEBITO CBD'
      ''
      
        'WHERE (CBD.data_movimento BETWEEN :pDataInicial AND :pDataFinal)' +
        ' AND'
      '      (CBD.id_organizacao = :PIDORGANIZACAO) AND'
      '      (CBD.id_conta_bancaria = :PIDCONTA)'
      ''
      'GROUP BY CBD.data_movimento')
    Left = 688
    Top = 552
