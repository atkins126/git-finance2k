  object FDQuery1: TFDQuery
    SQL.Strings = (
      'SELECT  CBT.ID_CONTA_BANCARIA_TRANSFERENCIA,'
      '        CBT.ID_CONTA_BANCARIA_CREDITO,'
      '        CBT.ID_CONTA_BANCARIA_DEBITO,'
      '        CBT.ID_TIPO_OPERACAO_BANCARIA,'
      '        CBT.VALOR, CBT.DATA_MOVIMENTO,'
      '        CBT.IDENTIFICACAO'
      ''
      'FROM CONTA_BANCARIA_TRANSFERENCIA CBT'
      ''
      
        'LEFT OUTER JOIN CONTA_BANCARIA_CREDITO CBC ON (CBC.ID_CONTA_BANC' +
        'ARIA_CREDITO = CBT.ID_CONTA_BANCARIA_CREDITO)'
      ''
      'WHERE   (CBT.ID_ORGANIZACAO = :PIDORGANIZACAO) AND'
      
        '        (CBT.DATA_MOVIMENTO BETWEEN :DTDATAINICIAL AND :DTDATAFI' +
        'NAL) AND'
      '        (CBC.ID_LOTE_CONTABIL IS NULL)'
      ''
      ''
      ''
      ''
      ''
      ''
      ''
      ''
      ''
      '')
    Left = 136
    Top = 128
    ParamData = <
      item
        Name = 'PIDORGANIZACAO'
        DataType = ftString
        ParamType = ptInput
        Size = 36
      end
      item
        Name = 'DTDATAINICIAL'
        DataType = ftDate
        ParamType = ptInput
      end
      item
        Name = 'DTDATAFINAL'
        DataType = ftDate
        ParamType = ptInput
      end>
  end
  object qryObterPorPeriodo: TFDQuery
    Connection = dmConexao.Conn
    SQL.Strings = (
      'SELECT CBD.ID_CONTA_BANCARIA,'
      '       CBD.ID_TIPO_OPERACAO_BANCARIA,'
      '       CBD.VALOR, CBD.DESCRICAO,'
      '       CBD.DATA_MOVIMENTO,'
      '       CBD.IDENTIFICACAO,'
      '       CONTA.CONTA AS CONTA_CORRENTE_DEBITO,'
      '       CC.CONTA AS CONTA_CONTABIL_DEBITO,'
      '       CC.CODREDUZ AS COD_REDUZ_DEBITO,'
      '       '#39'CBT'#39' AS TIPO'
      ''
      ''
      'FROM CONTA_BANCARIA_DEBITO CBD'
      
        'LEFT OUTER JOIN CONTA_BANCARIA CONTA ON (CONTA.ID_CONTA_BANCARIA' +
        ' = CBD.ID_CONTA_BANCARIA)'
      
        'LEFT OUTER JOIN CONTA_CONTABIL CC ON (CC.ID_CONTA_CONTABIL = CON' +
        'TA.ID_CONTA_CONTABIL)'
      ''
      'WHERE (CBD.ID_LOTE_CONTABIL IS NULL) AND'
      '(CBD.ID_ORGANIZACAO = :PIDORGANIZACAO) AND'
      '(CBD.DATA_MOVIMENTO BETWEEN :DTDATAINICIAL AND :DTDATAFINAL) AND'
      '(CBD.ID_TIPO_OPERACAO_BANCARIA = :PIDTIPOOPERACAO)')
    Left = 96
    Top = 96
    ParamData = <
      item
        Name = 'PIDORGANIZACAO'
        DataType = ftString
        ParamType = ptInput
        Size = 36
      end
      item
        Name = 'DTDATAINICIAL'
        DataType = ftDate
        ParamType = ptInput
      end
      item
        Name = 'DTDATAFINAL'
        DataType = ftDate
        ParamType = ptInput
      end
      item
        Name = 'PIDTIPOOPERACAO'
        DataType = ftString
        ParamType = ptInput
        Size = 36
      end>
  end
end
