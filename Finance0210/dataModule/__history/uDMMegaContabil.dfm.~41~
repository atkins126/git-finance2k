  object qryObterPlanoContas: TFDQuery
    Connection = dmConexao.ConnMega
    FetchOptions.AssignedValues = [evRowsetSize]
    FetchOptions.RowsetSize = 500000
    SQL.Strings = (
      'SELECT '
      '    NMCONTA,'
      '    CONTA, '
      '    DGVER, '
      '    CODREDUZ, '
      '    DGREDUZ, '
      '    INSCMF, '
      '    TIPO, '
      '    GRAU, '
      '    ORDEM_DIPJ, '
      '    RELACIONAMENTO, '
      '    NATUREZA '
      'FROM CPLANOEMPRESA '
      
        'WHERE (LEI11638 <> 0 ) AND (GRAU = 5)  AND (EMPRESA = :PIDEMPRES' +
        'A) ORDER BY CONTA')
