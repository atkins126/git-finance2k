  object qryConsultaContaContabil: TFDQuery
    Connection = dmConexao.ConnMega
    FetchOptions.AssignedValues = [evRowsetSize]
    FetchOptions.RowsetSize = 500000
    SQL.Strings = (
      'SELECT '
      '    NMCONTA,'
      '    CONTA, '
      '    DGVER, '
      '    CODREDUZ, '
      '    DGREDUZ, '
      '    INSCMF, '
      '    TIPO, '
      '    GRAU, '
      '    ORDEM_DIPJ, '
      '    RELACIONAMENTO, '
      '    NATUREZA '
      'FROM CPLANOEMPRESA '
      'WHERE CONTA = :PCONTA'
      'AND CODREDUZ = :PCDREDUZ'
      'AND (LEI11638 <> 0 )'
      'AND (GRAU = 5)'
      'AND (EMPRESA = :PIDEMPRESA)')
    Left = 464
    Top = 144
    ParamData = <
      item
        Name = 'PCONTA'
        DataType = ftString
        ParamType = ptInput
      end
      item
        Name = 'PCDREDUZ'
        DataType = ftInteger
        ParamType = ptInput
      end
      item
        Name = 'PIDEMPRESA'
        DataType = ftInteger
        ParamType = ptInput
      end>
