  object qryVerificaFechamento: TFDQuery
    Connection = dmConexao.ConnMega
    SQL.Strings = (
      'SELECT '
      '  L.ID'
      ''
      'FROM'
      '  CLANCAMENTOS L'
      ''
      'WHERE'
      '  (L.EMPRESA = :pIDEmpresa) AND'
      '  ((L.CDHIST = 65) OR (L.CDHIST = 66) OR'
      '   (L.CDHIST = 75) OR (L.CDHIST = 76)) AND'
      '(L.DATA BETWEEN :pDataInicial AND '#39'31.12.2020'#39')'
      ''
      '')
    Left = 328
    Top = 40
    ParamData = <
      item
        Name = 'PIDEMPRESA'
        ParamType = ptInput
      end
      item
        Name = 'PDATAINICIAL'
        ParamType = ptInput
      end>
  end
